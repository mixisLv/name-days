{
    "name": "mixislv/name-days",
    "description": "Latvian Name Days",
    "type": "library",
    "license": "MIT",
    "keywords": [ "php", "namedays", "latvian" ],
    "authors": [
        {
            "name": "Mikus Rozenbergs",
            "email": "mikus.rozenbergs@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "^9.2",
        "php-coveralls/php-coveralls": "^2.7",
        "friendsofphp/php-cs-fixer": "^3.51",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "mixisLv\\NameDays\\": "src"
        },
        "autoload-dev": {
            "psr-4": {
                "mixisLv\\NameDays\\Tests\\": "tests"
            }
        }
    }
}
